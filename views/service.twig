{% extends "base.twig" %}

{% block content %}
	<!-- Page Header -->
	<header class="intro-header" style="">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
					<div class="page-heading">
						<h1>
							{{service.title}}
						</h1>
						{% if service.subtitle %}
							<hr class="small">
							<span class="meta">
								{{ service.subtitle }}
							</span>
						{% endif %}
					</div>

				</div>
			</div>
		</div>
	</header>
	<!-- Post Content -->
	<article>
		<div class="container">
			<div class="row">
				<div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">

					{{ TimberImage(post.gallery[1]) }}
					{{service.content}}
				</div>
			</div>
		</div>
	</article>
	{% include "contact.twig" %}

{% endblock %}
