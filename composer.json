{
  "name": "dangreenisrael/trampoline-agency",
  "description": "Trampoline starter theme",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
      {
          "name": "Dan Green",
          "email": "dan@trampolinedigital.com"
      }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/roborourke/wp-less.git"
    }
  ],
  "extra": {
  	"installer-paths": {
      "./vendor/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
  	}
	},
  "require": {
    "timber/timber"  : "^1.1",
    "icit/wp-less"   : "dev-master",
    "aristath/kirki" : "2.3.2",
    "trampoline-digital/wp-scss": "dev-master"
  }
}
